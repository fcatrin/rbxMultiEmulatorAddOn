<?eclipse.ant.import?>
<project name="retroarch-jni" default="build-jni" basedir=".">
<property file="../../../retrox.properties"/>

<target name="build-jni">
    <exec executable="${ndk.dir}/ndk-build" failonerror="true"/>
</target>

<target name="clean">
    <exec executable="${ndk.dir}/ndk-build" failonerror="true">
        <arg value="clean"/>
    </exec>
</target>
</project>